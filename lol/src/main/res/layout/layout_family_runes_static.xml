<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/precisionPrimaryRunes"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:visibility="visible">


    <ImageView
        android:id="@+id/family1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        android:src="@drawable/precision"
        app:layout_constraintEnd_toStartOf="@+id/family2"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/family2" />

    <ImageView
        android:id="@+id/family2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        android:src="@drawable/domination"
        app:layout_constraintEnd_toStartOf="@+id/family3"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/family1"
        app:layout_constraintTop_toTopOf="@+id/family3" />

    <ImageView
        android:id="@+id/family3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        android:src="@drawable/sorcery"
        app:layout_constraintEnd_toStartOf="@+id/family4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/family2"
        app:layout_constraintTop_toTopOf="@+id/family4" />

    <ImageView
        android:id="@+id/family4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:contentDescription="@string/todo"
        android:src="@drawable/resolve"
        app:layout_constraintEnd_toStartOf="@+id/family5"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/family3"
        app:layout_constraintTop_toTopOf="@+id/family5" />

    <ImageView
        android:id="@+id/family5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:contentDescription="@string/todo"
        android:src="@drawable/inspiration"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/family4" />


</androidx.constraintlayout.widget.ConstraintLayout>