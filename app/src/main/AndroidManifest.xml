<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="pocketspace.metamanager">

    <application
        android:icon="@mipmap/hat_ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/hat_ic_launcher_round"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">

        <activity
            android:name=".ActivityGameList"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ActivityAboutPage"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".ActivityCharacterList"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".ActivityRole"
            android:theme="@style/AppTheme"/>

        <activity
            android:name=".ActivityCharacter"
            android:theme="@style/AppTheme"/>

    </application>

</manifest>